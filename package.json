{
  "name": "img2ico",
  "version": "1.2.0",
  "description": "A tool for converting image to ICO format.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nini22P/img2ico.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "img2ico": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:wasm && tsc && npm run copy:wasm",
    "build:wasm": "npm run build:wasm:node && npm run build:wasm:web",
    "build:wasm:node": "wasm-pack build ./wasm --target nodejs --out-dir ../src/wasm-node",
    "build:wasm:web": "wasm-pack build ./wasm --target web --out-dir ../src/wasm-web",
    "copy:wasm": "node copy-wasm.mjs",
    "build:web": "npm run build:wasm && vite build web",
    "prepublishOnly": "npm run build",
    "web": "npm run build:wasm && vite web",
    "lint": "eslint"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "jimp": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "buffer": "^6.0.3",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6",
    "vite-plugin-compression": "^0.5.1",
    "wasm-pack": "^0.13.1"
  },
  "keywords": [
    "ico",
    "image",
    "converter",
    "cli"
  ],
  "author": "22",
  "license": "MIT"
}
